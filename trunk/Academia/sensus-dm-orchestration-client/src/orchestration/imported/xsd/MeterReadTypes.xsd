					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="30">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_30_MINUTES" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="60">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_60_MINUTES" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="120">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_120_MINUTES" />
