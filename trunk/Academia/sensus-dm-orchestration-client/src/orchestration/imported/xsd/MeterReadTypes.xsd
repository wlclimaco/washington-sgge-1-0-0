	    	<xs:enumeration value="0">
				<xs:annotation>
	           		<xs:appinfo>
	                	<jaxb:typesafeEnumMember name="NA"/>
	              	</xs:appinfo>
	          	</xs:annotation>
	   		</xs:enumeration>
	     	<xs:enumeration value="1">
				<xs:annotation>
	           		<xs:appinfo>
	                	<jaxb:typesafeEnumMember name="INTERVAL_1_MINUTES"/>
	              	</xs:appinfo>
	          	</xs:annotation>
	   		</xs:enumeration>
