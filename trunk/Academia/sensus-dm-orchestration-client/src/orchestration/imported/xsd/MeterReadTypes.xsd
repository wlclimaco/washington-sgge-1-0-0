<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.sensus.com/common/messagetypes/meterread"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"  elementFormDefault="qualified"
        xmlns:tns="http://www.sensus.com/common/messagetypes/meterread"
        xmlns:meter="http://www.sensus.com/common/messagetypes/meter"
        xmlns:meas="http://www.sensus.com/common/messagetypes/measurements"
        xmlns:read="http://www.sensus.com/common/messagetypes/read"
        xmlns:status="http://www.sensus.com/common/messagetypes/read/status"
        xmlns:missing="http://www.sensus.com/common/messagetypes/read/missing"
        xmlns:svc="http://www.sensus.com/common/messagetypes/service" 
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
   		xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" jaxb:version="2.0"
   		jaxb:extensionBindingPrefixes="xjc">
					   		
	<xs:import schemaLocation="MeterIdentity.xsd"
		namespace="http://www.sensus.com/common/messagetypes/meter" />
		
	<xs:import schemaLocation="ServiceType.xsd" 
       namespace="http://www.sensus.com/common/messagetypes/service" />

		
	<xs:import schemaLocation="Measurements.xsd" 
       namespace="http://www.sensus.com/common/messagetypes/measurements" />

	       
    <xs:import schemaLocation="ReadTypes.xsd" 
       namespace="http://www.sensus.com/common/messagetypes/read" />
       
    <xs:import schemaLocation="CommonReadStatus.xsd" 
       namespace="http://www.sensus.com/common/messagetypes/read/status" />
		 
    <xs:import schemaLocation="MissingReadStatus.xsd" 
       namespace="http://www.sensus.com/common/messagetypes/read/missing" />
	
	<xs:complexType name="IntervalRead">
        <xs:annotation>
			<xs:documentation>Common Interval Read Object</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element  name="intervalPeriod" type="tns:IntervalPeriod">
				<xs:annotation>
					<xs:documentation>
						Enumerated type that identifies the sample interval duration 
						There is only a fixed number of interval duration options.
					</xs:documentation>
		        </xs:annotation>
		    </xs:element>
			<xs:element minOccurs="0" maxOccurs="1" name="timeOfIntercept" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>
						This is the time that the actual flexnet read message 
						arrived at the TGB (Tower Gateway Base station)
					</xs:documentation>
		        </xs:annotation>
		    </xs:element>
			<xs:element name="meterRead" type="tns:MeterRead" maxOccurs="unbounded" />						
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="MeterRead">
        <xs:annotation>
			<xs:documentation>Common Meter Read Object</xs:documentation>
        </xs:annotation>
		<xs:sequence>						
		   <xs:element  name="dataSetId" type="xs:int"> 
				<xs:annotation>
					<xs:documentation>
						DataSet Id of the meter read data. ESM enabled meters can have multiple data-sets configured per meter 
						and meter read data is collected per data set.
						Non-ESM (Legacy) meters have single data-set configured per meter.
					</xs:documentation>
		        </xs:annotation>
		    </xs:element>	
		    <xs:element name="isLPRead" type="xs:boolean" nillable="true">
				<xs:annotation>
					<xs:documentation>
						Boolean to represent whether or NOT the meter read is from Load Profile (LP) data.
						Enhanced Supervisory Read Message (ESM)is type of LPRead.
						Non-ESM read message is not type of LPRead.						
					</xs:documentation>
		        </xs:annotation>
		    </xs:element>	
			<xs:element minOccurs="0" name="sampleTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>
						Actual sample time is only present at the meter message transmit level 
						There is NO actual sample time for every interval (ie. packed history)
					</xs:documentation>
		        </xs:annotation>
			</xs:element>
			<xs:element name="samplePoint" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>
						The sample point is the time of sample measurement.
						The sample point assumes fixed points based on fixed interval durations.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="isEndReading" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>
						Boolean to represent whether or NOT the object refers to a "current read"
						record where the sampleTime, demand measurements, voltage measurements
					</xs:documentation>
		        </xs:annotation>
		    </xs:element>
			<xs:element name="commonStatus" type="status:CommonReadStatus" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>
						This represents common meter interval status at the time of interval recording.
						Common Status will only be present when an irregularity occurs.
						ESM read messages can have multiple Common Status values.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="missingReadStatus" type="missing:MissingReadStatus">
				<xs:annotation>
					<xs:documentation>
						This represents the missing status of this read entry.  It indicates if the read is 
						a missing read place holder as well has clarifies details about why the read may be
						missing.
					</xs:documentation>
				</xs:annotation>
			</xs:element>			
			<xs:element maxOccurs="unbounded" name="channels" type="tns:Channel">
				<xs:annotation>
					<xs:documentation>
						This List contains all readings recorded by the meter during an interval.
						Common Status will only be present when an irregularity occurs.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="TierRead">
        <xs:annotation>
			<xs:documentation>Common Tier Read Object</xs:documentation>
        </xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="1" maxOccurs="unbounded" name="tiers" type="tns:TierValue">
				<xs:annotation>
					<xs:documentation>
						This List contains all readings recorded by the meter during an interval.
						Common Status will only be present when an irregularity occurs.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TierValue">
		<xs:annotation>
			<xs:documentation>
				Tier Reading object that is self describing for the measured quantity.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="1" maxOccurs="1" name="tierId" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Units of the measured quantity.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="timeOfInsertion" type="xs:dateTime"></xs:element>
			<xs:element minOccurs="1" maxOccurs="unbounded" name="tierMeasuredQuantities" type="tns:TierMeasurementValue">
				<xs:annotation>
					<xs:documentation>
						This List contains all tier quantities.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	
	<xs:complexType name="TierMeasurementValue">
		<xs:annotation>
			<xs:documentation>
				Tier Reading object that is self describing for the measured quantity.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="1" maxOccurs="1" name="type" type="tns:ChannelType">
				<xs:annotation>
					<xs:documentation>
						Type of measured quantity that carries NO units.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="1" maxOccurs="1" name="unit" type="tns:UnitOfMeasure">
				<xs:annotation>
					<xs:documentation>
						Units of the measured quantity.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="1" maxOccurs="1" name="quantityType" type="meas:QuantityType">
			<xs:annotation>
					<xs:documentation>
						Quantity type, such as summation, interval, peak_demand, value
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="1" maxOccurs="1" name="value" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						The value of the sample point.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	
	<xs:complexType name="Channel">
	<xs:annotation>
		<xs:documentation>
			Reading object that is self describing for the
			measured quantity.
		</xs:documentation>
	</xs:annotation>
	<xs:sequence>
		<xs:element name="id" type="xs:short" minOccurs="0">
			<xs:annotation>
				<xs:documentation>
					Channel id
				</xs:documentation>
			</xs:annotation>
		</xs:element>
		
		<xs:element name="type" type="tns:ChannelType">
			<xs:annotation>
				<xs:documentation>
					Type of measured quantity that carries NO units.
					</xs:documentation>
			</xs:annotation>
		</xs:element>
		
		<!-- Start New ESM based API attributes -->
		<xs:element name="timeAttribute" type="tns:TimeAttribute">
			<xs:annotation>
				<xs:documentation>
					This attribute indicates the time aspect of the measured quantity. 
					</xs:documentation>
			</xs:annotation>
		</xs:element>			
		<xs:element name="qualifier" type="meas:DataQualifier">
			<xs:annotation>
				<xs:documentation>
					Indicates the salient feature of the measured quantity.
					Some examples are, maximum, minimum and average.
					</xs:documentation>
			</xs:annotation>
		</xs:element>	
		<xs:element  name="quantityType"
			type="meas:QuantityType">
			<xs:annotation>
				<xs:documentation>
					Quantity type, such as summation, interval,
					peak_demand, value
				</xs:documentation>
			</xs:annotation>
		</xs:element>
		<xs:element name="netFlowType" type="meas:NetFlowType">
			<xs:annotation>
				<xs:documentation>
					Indicates the flow direction of the measured quantity.
					</xs:documentation>
			</xs:annotation>
		</xs:element>		
		<xs:element name="segmentation" type="meas:Segmentation">
			<xs:annotation>
				<xs:documentation>
					Indicates the phase related information of the measured quantity
					</xs:documentation>
			</xs:annotation>
		</xs:element>		
		<xs:element name="multiplier" type="meas:Multiplier">
			<xs:annotation>
				<xs:documentation>
					Indicates the decimal scaling value to apply to the value of measured quantity
					</xs:documentation>
			</xs:annotation>
		</xs:element>
		<xs:element  name="unit" type="tns:UnitOfMeasure">
			<xs:annotation>
				<xs:documentation>
					Units of the measured quantity.
					</xs:documentation>
			</xs:annotation>
		</xs:element>		
		<xs:element name="value" type="xs:string">
			<xs:annotation>
				<xs:documentation>
					The value of the sample point for the given
					quantity type
				</xs:documentation>
			</xs:annotation>
		</xs:element>		
			<xs:element name="captureTime" type="xs:long" minOccurs="0" >
			<xs:annotation>
				<xs:documentation>
					The time (epoch value in seconds) when the value for the given quantity type is captured.
					It is an optional field, mainly used for snapshot (such as demand) values.
				</xs:documentation>
			</xs:annotation>
		</xs:element>		
		<xs:element name="channelStatus" type="read:ReadingValueStatus">
			<xs:annotation>
				<xs:documentation>
					The channel status will always be present.
					The
					status is representative of individual quantities measured.
				</xs:documentation>
			</xs:annotation>
		</xs:element>
		<!-- End ESM based API attributes -->
	</xs:sequence>
</xs:complexType>
	
	<xs:simpleType name="ChannelType">
		<xs:annotation>
			<xs:documentation>
				Measured quantities are described without units.
				Strict C12.19 is not followed here.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
		<!-- Start Legacy Types  -->
			<xs:enumeration value="Peak Demand" />
			<xs:enumeration value="NET Summation (F+R)" />
			<xs:enumeration value="Simple NET (F-R)" />
			<xs:enumeration value="Delivered (R)" />
			<xs:enumeration value="Consumed (F)" />
			<xs:enumeration value="Corrected Volume" />
			<xs:enumeration value="Uncorrected Volume" />
			<xs:enumeration value="Gallons" />
			<xs:enumeration value="Min Voltage" />
			<xs:enumeration value="Max Voltage" />
			<xs:enumeration value="Avg Voltage" />
			<xs:enumeration value="Phase A Voltage" />
			<xs:enumeration value="Phase B Voltage" />
			<xs:enumeration value="Phase C Voltage" />
		<!-- End Legacy Types  -->
		    <xs:enumeration value="ENERGY" />
			<xs:enumeration value="POWER" />
			<xs:enumeration value="PHASOR_REACTIVE_POWER" />
			<xs:enumeration value="QUANTITY_POWER" />
			<xs:enumeration value="DISTORTION_VOLT_AMPERES" />
			<xs:enumeration value="VOLTAGE" />
			<xs:enumeration value="VOLTAGE_FIXED_BLOCK" />
			<xs:enumeration value="CURRENT" />
			<xs:enumeration value="TIME" />
			<xs:enumeration value="DATE" />
			<xs:enumeration value="DATE_TIME" />
			<xs:enumeration value="VOLTAGE_PHASE_ANGLE" />
			<xs:enumeration value="CURRENT_PHASE_ANGLE" />
			<xs:enumeration value="POWER_FACTOR" />
			<xs:enumeration value="APPARENT_POWER_FACTOR" />
			<xs:enumeration value="PHASOR_POWER_FACTOR" />
			<xs:enumeration value="COUNTER" />
			<xs:enumeration value="LIQUID_VOLUME" />
			<xs:enumeration value="LIQUID_FLOW" />
			<xs:enumeration value="GAS_VOLUME" />
			<xs:enumeration value="GAS_FLOW" />
			<xs:enumeration value="TEMPRATURE" />
			<xs:enumeration value="DEMAND" />
			<xs:enumeration value="DEMAND_TIME" />
			<xs:enumeration value="PRESSURE" />
			<xs:enumeration value="FREQUENCY" />
			<xs:enumeration value="INSTRUMENTATION" />
		    
		</xs:restriction>
	</xs:simpleType>
	
