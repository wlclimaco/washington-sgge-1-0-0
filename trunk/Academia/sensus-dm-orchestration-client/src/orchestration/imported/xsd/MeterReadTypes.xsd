				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_10_MINUTES" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="15">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_15_MINUTES" />
