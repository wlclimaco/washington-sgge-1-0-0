						<jaxb:typesafeEnumMember name="INTERVAL_480_MINUTES" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="720">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_720_MINUTES" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1440">
				<xs:annotation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="INTERVAL_1440_MINUTES" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	
	
