<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="SensusAppContext" class="com.sensus.common.util.SensusAppContext" />

	<bean id="tenantDACTarget" class="com.sensus.dm.common.tenant.dac.mybatis.TenantDACImpl">
		<property name="sqlSessionFactory" ref="tenant-sqlSessionFactory" />
	</bean>

	<bean id="tenantDACImplTarget" class="com.sensus.dm.common.tenant.dac.TenantDACImplTest" />

	<!-- Setup for mybatis Database Layer GROUP -->
	<bean id="tenant-sqlSessionFactory" class="org.mybatis.spring.SqlSessionFactoryBean">
		<property name="configLocation"
			value="classpath:com/sensus/dm/common/tenant/dac/mybatis/map/dm-common-tenant-mybatis-config.xml" />
		<property name="dataSource" ref="epmDataSource" />
	</bean>

</beans>
