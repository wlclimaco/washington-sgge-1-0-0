<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="SensusAppContext" class="com.sensus.common.util.SensusAppContext"/>

	<bean id="noteDACImplTarget" class="com.sensus.dm.common.device.dac.NoteDACImplTest"/>

	<bean id="noteDACTarget" class="com.sensus.dm.common.device.dac.mybatis.NoteDACImpl">
		<property name="sqlSessionFactory" ref="note-sqlSessionFactory" />
	</bean>

	<bean id="note-sqlSessionFactory" class="org.mybatis.spring.SqlSessionFactoryBean">
		<property name="configLocation" value="classpath:com/sensus/dm/common/device/dac/mybatis/map/dm-note-mybatis-config.xml" />
		<property name="dataSource" ref="epmDataSource" />
	</bean>

</beans>
