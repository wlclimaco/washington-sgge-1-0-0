<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="SensusAppContext" class="com.sensus.common.util.SensusAppContext" />

	<bean id="processSummaryDACTarget" class="com.sensus.dm.common.process.dac.mybatis.ProcessSummaryDACImpl">
		<property name="sqlSessionFactory" ref="process-summary-sqlSessionFactory" />
	</bean>

	<bean id="processSummaryDACImplTarget" class="com.sensus.dm.common.process.dac.ProcessSummaryDACImplTest" />

	<bean id="process-summary-sqlSessionFactory" class="org.mybatis.spring.SqlSessionFactoryBean">
		<property name="configLocation"	value="classpath:com/sensus/dm/common/process/dac/mybatis/map/dm-common-process-summary-mybatis-config.xml" />
		<property name="dataSource" ref="epmDataSource" />
	</bean>

</beans>
