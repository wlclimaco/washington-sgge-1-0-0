<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="SensusAppContext" class="com.sensus.common.util.SensusAppContext"/>	
	
	<!-- Setup for mybatis Database Layer SETTINGS -->
	<bean id="settings-sqlSessionFactory" class="org.mybatis.spring.SqlSessionFactoryBean">
		<property name="configLocation"
			value="classpath:com/sensus/dm/elec/settings/dac/mybatis/map/dm-elec-settings-mybatis-config.xml" />
    <property name="dataSource" ref="epmDataSource" />
  </bean>
	
	<bean id="settingsDACTarget" class="com.sensus.dm.elec.settings.dac.mybatis.SettingsDACImpl">
		<property name="sqlSessionFactory" ref="settings-sqlSessionFactory" />
	</bean>
	
	<bean id="settingsDACImplTarget" class="com.sensus.dm.elec.settings.dac.SettingsDACImplTest" />
	
</beans>
