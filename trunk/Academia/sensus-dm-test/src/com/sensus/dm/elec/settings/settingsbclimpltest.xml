<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="SensusAppContext" class="com.sensus.common.util.SensusAppContext"/>

	<bean id="settingsBCLTarget" class="com.sensus.dm.elec.settings.bcl.impl.SettingsBCLImpl">
		<property name="settingsDAC" ref="settingsDACMockTarget"/>
		<property name="propertyBCL" ref="propertyBCLMockTarget"/>
		<property name="defaultLanguage" value="en_US" />
		<property name="defaultTimeZone" value="US/Eastern" />
		<property name="defaultDateFormat" value="MM/dd/yyyy" />
		<property name="defaultMonitorRequest" value="1" />
		<property name="defaultPageSize" value="25" />
		<property name="defaultTemperatre" value="FAHRENHEIT" />
		<property name="defaultShowDialogPolygon" value="true" />
	</bean>

</beans>
