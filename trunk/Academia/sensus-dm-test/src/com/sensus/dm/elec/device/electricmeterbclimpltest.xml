	<!-- Message Driven POJO (MDP) for MeterReads -->
	<bean id="intervalRead.notif.messageReceiver" class="com.sensus.dm.common.schedule.bcl.impl.MeterReadsReceiver"
		lazy-init="true" />

	<!-- Message Driven POJO (MDP) for Reading Changed Meter Reads -->
	<bean id="registerRead.notif.messageReceiver" class="com.sensus.dm.common.schedule.bcl.impl.ReadingChangedReceiver"
		lazy-init="true" />

	<!-- Message Driven POJO (MDP) for GlASS Read Meter Reads -->
	<bean id="glassRead.notif.messageReceiver" class="com.sensus.dm.common.schedule.bcl.impl.GlassReadReceiver"
		lazy-init="true">
		<property name="useGatewayCacheForGlassRead">
			<value>false</value>
		</property>
	</bean>

	<!-- The following map is used to translate from a multispeak field name
		to a reading type selector. -->
	<util:map id="fieldNameToReadingTypeSelector" map-class="java.util.HashMap"
		key-type="java.lang.String"
		value-type="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
		<entry key="maxDemand">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="quantityType" value="PEAK_DEMAND"></property>
			</bean>
		</entry>
		<entry key="posKWh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="FORWARD_ONLY"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="ACTIVE_POWER"></property>
			</bean>
		</entry>
		<entry key="negKWh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="REVERSE_ONLY"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="ACTIVE_POWER"></property>
			</bean>
		</entry>
		<entry key="netKWh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="SIMPLE_NET"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="ACTIVE_POWER"></property>
			</bean>
		</entry>
		<entry key="totalKWh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="REVERSE_ENERGY_DETECTION"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="ACTIVE_POWER"></property>
			</bean>
		</entry>
		<entry key="posKVARh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="FORWARD_ONLY"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="REACTIVE_POWER"></property>
			</bean>
		</entry>
		<entry key="netKVARh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="SIMPLE_NET"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="REACTIVE_POWER"></property>
