				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="APPARENT_POWER"></property>
			</bean>
		</entry>
		<entry key="CorrGasVol"> <!-- OR CUBIC_FEET_VOLUME_CORRECTED_PRESSURE_BASE ?? or cubit meter ? -->
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
