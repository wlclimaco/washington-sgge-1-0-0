				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="APPARENT_POWER"></property>
			</bean>
		</entry>
		<entry key="negKVAh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="REVERSE_ONLY"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="APPARENT_POWER"></property>
			</bean>
		</entry>
		<entry key="netKVAh">
			<bean class="com.sensus.api.getmeterreadings.messages.ReadingTypeSelector">
				<property name="netFlowType" value="SIMPLE_NET"></property>
				<property name="quantityType" value="INTERVAL"></property>
				<property name="unitOfMeasoure" value="APPARENT_POWER"></property>
			</bean>
		</entry>
		<entry key="totalKVAh">
