<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>

	<typeAliases>
		<typeAlias alias="Tag"  type="com.sensus.dm.common.tag.model.Tag"/>
		<typeAlias alias="TagRequest"  type="com.sensus.dm.common.tag.model.request.TagRequest"/>
		<typeAlias alias="InquiryTagRequest" type="com.sensus.dm.common.tag.model.request.InquiryTagRequest" />
		<typeAlias alias="Device" type="com.sensus.cbof.model.Device" />
		<typeAlias alias="Group" type="com.sensus.dm.common.group.model.Group" />
		<typeAlias alias="GroupRequest" type="com.sensus.dm.common.group.model.request.GroupRequest" />
		<typeAlias alias="InquiryGroupRequest" type="com.sensus.dm.common.group.model.request.InquiryGroupRequest" />
	</typeAliases>

	<mappers>
    	<mapper resource="com/sensus/dm/common/tag/dac/mybatis/map/tag.xml"/>
    	<mapper resource="com/sensus/dm/common/group/dac/mybatis/map/group.xml" />
    </mappers>

</configuration>
