	     #### ELECTRIC DEFAULT ACTIONS ####
	     ##################################
	-->
	<bean id="electricMeterDefaultActionsTarget" class="com.sensus.dm.common.base.model.DefaultActionsPermissions">
		<property name="actions" >
			<list value-type="com.sensus.dm.common.action.model.ActionType" >
				<!-- Order by Category Action (Front-end grouping) -->
				<ref bean="initiateDemandResetAction"/>
				<ref bean="getConnectionStatusAction"/>
				<ref bean="getDemandResponseEventStatusAction"/>
				<ref bean="demandResponseAction"/>
				<ref bean="sendHanTextMessageAction"/>
				<ref bean="getRemoteConnectStatusAction"/>
				<ref bean="remoteArmConnectAction"/>
				<ref bean="remoteConnectAction"/>
				<ref bean="remoteDisconnectAction"/>
			</list>
		</property>
	</bean>

	<bean id="hanDeviceDefaultActionsTarget" class="com.sensus.dm.common.base.model.DefaultActionsPermissions">
		<property name="actions" >
			<list value-type="com.sensus.dm.common.action.model.ActionType">
				<!-- Order by Category Action (Front-end grouping) -->
				<ref bean="getConnectionStatusAction"/>
				<ref bean="getDemandResponseSetupStatus"/>
