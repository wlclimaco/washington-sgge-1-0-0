	     ##############################
	     #### ELECTRIC DEVICE TYPE ####
	     ##############################
	-->
	<bean id="electricMeterDeviceTypeTarget" class="com.sensus.dm.common.base.model.DeviceTypePermissions">
		<property name="deviceType">
			<value>ELECTRIC_METER</value>
		</property>
		<property name="deviceTypeModels">
			<list>
				<ref bean="electricMeterCommonDeviceTypeModelTarget"/>
		  	</list>
		</property>
		<property name="defaultActions" ref="electricMeterDefaultActionsTarget" />
	</bean>

