	 -->
	<util:list id="cathodicGasEventHistoryFilterActionsTarget" list-class="java.util.ArrayList" value-type="com.sensus.cp.permissions.model.ActionType">

		<!-- Alert Category -->
		<ref bean="clearAllAlertsAction"/>
		<ref bean="clearAlertAction"/>
		<ref bean="deleteAlertAction"/>
		<ref bean="followUpAlertAction"/>
		<ref bean="serviceOrderAlertAction"/>

		<!-- Survey Category -->
		<ref bean="onDemandSurveyAction"/>
		<ref bean="manualSurveyAction"/>
		<ref bean="pspOnAction"/>
		<ref bean="pspOffAction"/>
		<ref bean="psp100mvPolarizationAction"/>

		<!-- Tag Category -->
		<ref bean="createTagAction"/>
		<ref bean="removeTagAction"/>
		<ref bean="addAssetTagAction"/>
		<ref bean="deleteAssetTagAction"/>

	</util:list>

