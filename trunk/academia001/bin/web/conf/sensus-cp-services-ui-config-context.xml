	<!-- Injected at Device BCL Layer -->
	<bean id="servicesTarget" class="com.sensus.cp.common.model.ServicesPermissions">
		<property name="servicesTypePermissions">
			<list>
	            <ref bean="cathodicGasServicesTypeTarget" />
			</list>
		</property>
	</bean>

