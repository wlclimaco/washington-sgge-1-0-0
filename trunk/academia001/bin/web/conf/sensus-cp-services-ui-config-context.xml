	     ###########################################
	     #### CATHODIC GAS DASHBOARD UI MODULES ####
	     ###########################################
	 -->
	<util:list id="dashboardCathodicGasTarget" list-class="java.util.ArrayList" value-type="java.lang.String">
		<value>map</value>
		<value>divisions</value>
		<value>protectionGraph</value>
	</util:list>

