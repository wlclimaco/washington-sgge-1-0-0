	     ##################
	     #### SERVICES ####
	     ##################
	-->

	<bean id="cathodicGasServicesTypeTarget" class="com.sensus.cp.common.model.ServicesTypePermissions">
		<property name="serviceTypeEnum"    value="CATHODIC_GAS"/>
		<property name="grantedAuthority"   value="ROLE_CP_SERVICE_CATHODIC_GAS"/>
		<property name="deviceTypePermissions">
			<list>
	            <ref bean="pipeSectionTarget" />
	            <ref bean="rectifierDeviceTypeTarget" />
	            <ref bean="testPointDeviceTypeTarget" />
	            <ref bean="alertTarget" />
			</list>
		</property>
		<property name="dashboardUiModules"        ref="dashboardCathodicGasTarget"/>
		<property name="eventHistoryFilterActions" ref="cathodicGasEventHistoryFilterActionsTarget"/>
	</bean>

