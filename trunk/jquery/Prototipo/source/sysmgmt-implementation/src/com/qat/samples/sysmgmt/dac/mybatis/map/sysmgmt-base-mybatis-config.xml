     	<mapper resource="com/qat/samples/sysmgmt/cidade/dac/mybatis/map/cidade.xml"/>
     	<mapper resource="com/qat/samples/sysmgmt/produto/dac/mybatis/map/produto.xml"/>
     	<mapper resource="com/qat/samples/sysmgmt/dac/mybatis/map/controleAcess.xml"/>
     	<mapper resource="com/qat/samples/sysmgmt/dac/mybatis/map/endereco.xml"/>
     	<mapper resource="com/qat/samples/sysmgmt/dac/mybatis/map/documento.xml"/>
     	<mapper resource="com/qat/samples/sysmgmt/supermercado/dac/mybatis/map/supermercado.xml"/>
    </mappers>

</configuration>
