  object IBUpdateSQL1: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  COD_PRODUTO,'
      '  UNID,'
      '  COD_TIPO_ITEM,'
      '  COD_PROD_ESP,'
      '  DESCR_PRODUTO,'
      '  COD_BARRA,'
      '  COD_ANT_ITEM,'
      '  TIPO_ITEM,'
      '  COD_NCM,'
      '  EX_IPI,'
      '  COD_GEN,'
      '  COD_LST,'
      '  ALIQ_ICMS,'
      '  MVA,'
      '  GRUPO,'
      '  SUBGRUPO,'
      '  INTEIRO,'
      '  CD_TRIBUTACAO,'
      '  ORIGEM,'
      '  OPERACAO,'
      '  RED_BASE_CALC,'
      '  ALIQ_CONFINS,'
      '  ALIQ_PIS,'
      '  VLAQUIS,'
      '  MARGEMPR,'
      '  ESTOQUE,'
      '  PRECOVENDA,'
      '  REF,'
      '  STATUS,'
      '  DERIVACAO,'
      '  COMPNCM,'
      '  CODSERV,'
      '  IPI'
      'from PROD_SERV '
      'where'
      '  COD_PRODUTO = :COD_PRODUTO')
    ModifySQL.Strings = (
      'update PROD_SERV'
      'set'
      '  UNID = :UNID,'
      '  COD_TIPO_ITEM = :COD_TIPO_ITEM,'
      '  COD_PROD_ESP = :COD_PROD_ESP,'
      '  DESCR_PRODUTO = :DESCR_PRODUTO,'
      '  COD_BARRA = :COD_BARRA,'
      '  COD_ANT_ITEM = :COD_ANT_ITEM,'
      '  TIPO_ITEM = :TIPO_ITEM,'
      '  COD_NCM = :COD_NCM,'
      '  EX_IPI = :EX_IPI,'
      '  COD_GEN = :COD_GEN,'
      '  COD_LST = :COD_LST,'
      '  ALIQ_ICMS = :ALIQ_ICMS,'
      '  MVA = :MVA,'
      '  GRUPO = :GRUPO,'
      '  SUBGRUPO = :SUBGRUPO,'
      '  INTEIRO = :INTEIRO,'
      '  CD_TRIBUTACAO = :CD_TRIBUTACAO,'
      '  ORIGEM = :ORIGEM,'
      '  OPERACAO = :OPERACAO,'
      '  RED_BASE_CALC = :RED_BASE_CALC,'
      '  ALIQ_CONFINS = :ALIQ_CONFINS,'
      '  ALIQ_PIS = :ALIQ_PIS,'
      '  VLAQUIS = :VLAQUIS,'
      '  MARGEMPR = :MARGEMPR,'
      '  ESTOQUE = :ESTOQUE,'
      '  PRECOVENDA = :PRECOVENDA,'
      '  REF = :REF,'
      '  STATUS = :STATUS,'
      '  DERIVACAO = :DERIVACAO,'
      '  COMPNCM = :COMPNCM,'
      '  CODSERV = :CODSERV,'
      '  IPI = :IPI'
      'where'
      '  COD_PRODUTO = :OLD_COD_PRODUTO')
    InsertSQL.Strings = (
      'insert into PROD_SERV'
      
        '  (UNID, COD_TIPO_ITEM, COD_PROD_ESP, DESCR_PRODUTO, COD_BARRA, ' +
        'COD_ANT_ITEM, '
      
        '   TIPO_ITEM, COD_NCM, EX_IPI, COD_GEN, COD_LST, ALIQ_ICMS, MVA,' +
        ' GRUPO, '
      
        '   SUBGRUPO, INTEIRO, CD_TRIBUTACAO, ORIGEM, OPERACAO, RED_BASE_' +
        'CALC, ALIQ_CONFINS, '
      
        '   ALIQ_PIS, VLAQUIS, MARGEMPR, ESTOQUE, PRECOVENDA, REF, STATUS' +
        ', DERIVACAO, '
      '   COMPNCM, CODSERV, IPI)'
      'values'
      
        '  (:UNID, :COD_TIPO_ITEM, :COD_PROD_ESP, :DESCR_PRODUTO, :COD_BA' +
        'RRA, :COD_ANT_ITEM, '
      
        '   :TIPO_ITEM, :COD_NCM, :EX_IPI, :COD_GEN, :COD_LST, :ALIQ_ICMS' +
        ', :MVA, '
      
        '   :GRUPO, :SUBGRUPO, :INTEIRO, :CD_TRIBUTACAO, :ORIGEM, :OPERAC' +
        'AO, :RED_BASE_CALC, '
      
        '   :ALIQ_CONFINS, :ALIQ_PIS, :VLAQUIS, :MARGEMPR, :ESTOQUE, :PRE' +
        'COVENDA, '
      '   :REF, :STATUS, :DERIVACAO, :COMPNCM, :CODSERV, :IPI)')
    DeleteSQL.Strings = (
      'delete from PROD_SERV'
      'where'
      '  COD_PRODUTO = :OLD_COD_PRODUTO')
    Left = 664
    Top = 18
  end
