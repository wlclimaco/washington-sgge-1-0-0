  object IBUpdateSQL2: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  ID,'
      '  COD_PRODUTO,'
      '  COD_BARRA'
      'from COD_BARRA '
      'where'
      '  ID = :ID')
    ModifySQL.Strings = (
      'update COD_BARRA'
      'set'
      '  COD_PRODUTO = :COD_PRODUTO,'
      '  COD_BARRA = :COD_BARRA'
      'where'
      '  ID = :OLD_ID')
    InsertSQL.Strings = (
      'insert into COD_BARRA'
      '  (COD_PRODUTO, COD_BARRA)'
      'values'
      '  (:COD_PRODUTO, :COD_BARRA)')
    DeleteSQL.Strings = (
      'delete from COD_BARRA'
      'where'
      '  ID = :OLD_ID')
    Left = 404
    Top = 490
  end
