  object IBUpdateSQL1: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      'from clientes '
      'where'
      '  CDCLIENTE = :CDCLIENTE')
    ModifySQL.Strings = (
      'update clientes'
      'set'
      '  STATUS = :STATUS,'
      '  NOME = :NOME,'
      '  FANTASIA = :FANTASIA,'
      '  CGC = :CGC,'
      '  INCR = :INCR,'
      '  LOGRADOURO = :LOGRADOURO,'
      '  NUMERO = :NUMERO,'
      '  BAIRRO = :BAIRRO,'
      '  MUNICIPIO = :MUNICIPIO,'
      '  UF = :UF,'
      '  CEP = :CEP,'
      '  FONE = :FONE,'
      '  FONEFAX = :FONEFAX,'
      '  CEL = :CEL,'
      '  EMAIL = :EMAIL,'
      '  EMAIL2 = :EMAIL2,'
      '  SITE = :SITE,'
      '  OBS = :OBS'
      'where'
      '  CDCLIENTE = :OLD_CDCLIENTE')
    InsertSQL.Strings = (
      'insert into clientes'
      
        '  (STATUS, NOME, FANTASIA, CGC, INCR, LOGRADOURO, NUMERO, BAIRRO' +
        ', MUNICIPIO, '
      '   UF, CEP, FONE, FONEFAX, CEL, EMAIL, EMAIL2, SITE, OBS)'
      'values'
      
        '  (:STATUS, :NOME, :FANTASIA, :CGC, :INCR, :LOGRADOURO, :NUMERO,' +
        ' :BAIRRO, '
      
        '   :MUNICIPIO, :UF, :CEP, :FONE, :FONEFAX, :CEL, :EMAIL, :EMAIL2' +
        ', :SITE, '
      '   :OBS)')
    DeleteSQL.Strings = (
      'delete from clientes'
      'where'
      '  CDCLIENTE = :OLD_CDCLIENTE')
    Left = 661
    Top = 17
  end
