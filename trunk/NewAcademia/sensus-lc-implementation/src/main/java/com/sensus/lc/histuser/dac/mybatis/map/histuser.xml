<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="HistUser">
	<resultMap id="histUserResults" type="HistUser">
		<result property="cdHistUser" column="cdhistuser" />
		<result property="cdtable" column="cdtable" />
		<result property="id" column="id" />
		<result property="acao" column="acao" />
		<result property="createdate" column="create_date" typeHandler="com.sensus.lc.base.util.DateTimeZoneTypeHandler"/>
		<result property="createuser" column="create_user" />
		<result property="tenantid" column="tenant_id" />
	</resultMap>

	<!-- Insert HistUser -->
	<select id="insertHistUser" parameterType="Map" resultType="int" statementType="CALLABLE">

     { call ins_histuser(
     		#{cdtable,jdbcType=INTEGER},
            #{id,jdbcType=INTEGER},
            #{acao,jdbcType=INTEGER},
            #{create_user,jdbcType=VARCHAR},
            #{tenant_id,jdbcType=INTEGER}

       ) }

    </select >

</mapper>