		<typeAlias alias="AlertCommunicationMaintenanceRequest" type="com.sensus.lc.communication.model.request.AlertCommunicationMaintenanceRequest"/>

	</typeAliases>

	<mappers>
		<mapper resource="com/sensus/lc/communication/dac/mybatis/map/alertcommunication.xml" />
	</mappers>
</configuration>
