<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

 <settings>
  <setting name="lazyLoadingEnabled" value="false" />
 </settings>

<typeAliases>

  <!-- Group mapping -->
  <typeAlias alias="Tenant"                   type="com.sensus.lc.tenant.model.Tenant" />
  <typeAlias alias="HistUserRequest"             type="com.sensus.lc.histuser.model.request.HistUserRequest" />
  <typeAlias alias="InquiryHistUserRequest" type="com.sensus.lc.histuser.model.request.InquiryHistUserRequest" />
  <typeAlias alias="HistUser"                    type="com.sensus.lc.histuser.model.HistUser"/>


</typeAliases>

 <typeHandlers>
   <typeHandler javaType="Date" jdbcType="TIMESTAMP" handler="com.sensus.lc.base.util.DateTimeZoneTypeHandler"/>
    </typeHandlers>

 <mappers>
     <mapper resource="com/sensus/lc/histuser/dac/mybatis/map/histUser.xml"/>

</mappers>

</configuration>