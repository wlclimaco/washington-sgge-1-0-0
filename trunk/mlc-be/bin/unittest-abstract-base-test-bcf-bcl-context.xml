	 <bean id="marshaller" class="org.springframework.oxm.jaxb.Jaxb2Marshaller"
		p:contextPath="com.sensus.mlc.mlcserver.type" />

	 Configure Spring-WS client template
	 p:defaultUri="http://localhost:9090/mlc-gateway-ws/mlc-gateway-ws"
	<bean id="wsTemplate" class="org.springframework.ws.client.core.WebServiceTemplate"
		p:defaultUri="http://localhost:8083/gateway-mlc/mlc-gateway-ws/"
		p:marshaller-ref="marshaller" p:unmarshaller-ref="marshaller">
	</bean>
 -->
	<bean id="mlcGatewayWSClient" class="com.sensus.mlc.mlcserver.client.MlcServerClientMock" lazy-init="false"/>

	<bean id="incompleteData" class="java.lang.String">
        <constructor-arg>
        	<value>Incomplete Data</value>
        </constructor-arg>
    </bean>

	<!-- This is the User and Pwd for LC System activities -->
	<bean id="systemIdTarget" class="java.lang.String">
		<constructor-arg>
			<value>${system.user.name}</value>
		</constructor-arg>
	</bean>
	<bean id="systemPwdTarget" class="java.lang.String">
		<constructor-arg>
			<value>${system.user.pwd}</value>
		</constructor-arg>
	</bean>
</beans>
