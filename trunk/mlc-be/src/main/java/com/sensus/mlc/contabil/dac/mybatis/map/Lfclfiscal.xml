		SELECT  <include refid="allCustomListLfclfiscalColumns"/>
		  FROM lfclfiscal
		 WHERE name = #{lfclfiscal.name,jdbcType=VARCHAR}
		   AND tenant_id = #{tenant.id,jdbcType=INTEGER}
	
