<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Unidmed">
<resultMap id="UnidmedsResult" type="Unidmed">
</resultMap>
<sql id="allUnidmedColumms">
SELECT 
CDUNIDAD,
DSUNIDAD,
LISTINSALT,
</sql>
<!-- Fetch All UnidMeds -->
<select id="fetchAllUnidmeds" parameterType="Map" resultMap="UnidmedsResult">
SELECT  <include refid="allUnidmedColumms"/>
FROM UnidMed;   
</select>
<select id="fetchAllUnidmedById"  parameterType="Map" resultMap="UnidmedResult">
   select <include refid="allUnidmedColumms"/>
   from UnidMed e   
   where   
   e.codemp = #{codemp}  
</select>
