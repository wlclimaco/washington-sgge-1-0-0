<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Atributos">
<resultMap id="AtributossResult" type="Atributos">
</resultMap>
<sql id="allAtributosColumms">
SELECT 
nmtabela,
nmatribu,
dsatribu,
tpatribu,
dsmascar,
dshelp,
tmatribu,
nrseqatr,
</sql>
<!-- Fetch All atributoss -->
<select id="fetchAllAtributoss" parameterType="Map" resultMap="AtributossResult">
SELECT  <include refid="allAtributosColumms"/>
FROM atributos;   
</select>
<select id="fetchAllAtributosById"  parameterType="Map" resultMap="AtributosResult">
   select <include refid="allAtributosColumms"/>
   from atributos e   
   where   
   e.codemp = #{codemp}  
</select>
