<!-- Fetch All Filials -->
<select id="fetchAllFilials" parameterType="Map" resultMap="FilialsResult">
SELECT  <include refid="allFilialColumms"/>
FROM Filial;   
</select>
<select id="fetchAllFilialById"  parameterType="Map" resultMap="FilialResult">
   select <include refid="allFilialColumms"/>
   from Filial e   
   where   
   e.codemp = #{codemp}  
</select>
