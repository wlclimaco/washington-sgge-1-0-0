"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Empresa">
<resultMap id="EmpresasResult" type="Empresa">
</resultMap>
<sql id="allEmpresaColumms">
   SELECT 
     CODEMP,
     RAZEMP,
     NOMEEMP,
     CNPJEMP,
     INSCEMP,
     MULTIALMOXEMP,
     FOTOEMP,
     PERCISSEMP,
     CodEnd,
     NRCERTIF,
     NRSENCER,
     LISTINSALT,
   </sql>
<!-- Fetch All empresas -->
<select id="fetchAllEmpresas" parameterType="Map" resultMap="EmpresasResult">
   SELECT  <include refid="allEmpresaColumms"/>
       FROM empresa;   
   </select>
<select id="fetchAllEmpresaById"  parameterType="Map" resultMap="EmpresaResult">
   select <include refid="allEmpresaColumms"/>
       from empresa e   
       where   
       e.codemp = #{codemp}  
   </select>
