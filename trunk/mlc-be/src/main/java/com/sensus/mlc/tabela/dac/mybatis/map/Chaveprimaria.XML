<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Chaveprimaria">
<resultMap id="ChaveprimariasResult" type="Chaveprimaria">
</resultMap>
<sql id="allChaveprimariaColumms">
   SELECT 
     nmtabela,
     nmatribu,
     nrseqkey,
   </sql>
<!-- Fetch All chaveprimarias -->
<select id="fetchAllChaveprimarias" parameterType="Map" resultMap="ChaveprimariasResult">
   SELECT  <include refid="allChaveprimariaColumms"/>
       FROM chaveprimaria;   
   </select>
<select id="fetchAllChaveprimariaById"  parameterType="Map" resultMap="ChaveprimariaResult">
   select <include refid="allChaveprimariaColumms"/>
       from chaveprimaria e   
       where   
       e.codemp = #{codemp}  
   </select>
<!-- Insert Chaveprimaria -->
 <select id="insertChaveprimaria" parameterType="Chaveprimaria" resultType="int">
     SELECT ins_chaveprimaria (   
     #{nmtabela,jdbcType=VARCHAR},
     #{nmatribu,jdbcType=VARCHAR},
     #{nrseqkey,jdbcType=INTEGER},
 </select>
<!-- Update Chaveprimaria -->
 <select id="updateChaveprimaria" parameterType="Chaveprimaria" resultType="int">
     SELECT upd_chaveprimaria (   
     #{nmtabela,jdbcType=VARCHAR},
     #{nmatribu,jdbcType=VARCHAR},
     #{nrseqkey,jdbcType=INTEGER},
 </select>
</mapper>
