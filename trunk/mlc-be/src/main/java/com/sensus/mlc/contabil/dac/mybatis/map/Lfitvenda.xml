	<!-- Delete Lfitvenda -->
	<select id="deleteLfitvenda" parameterType="Lfitvenda">
		SELECT del_lfitvenda (#{id})
	</select>
	
	<!-- Fetch All Lfitvendas -->
	<select id="fetchAllLfitvendas" parameterType="Map" resultMap="listAllLfitvendaResults">
	
		SELECT  <include refid="allLfitvendaColumms"/>
		  FROM lfitvenda
	
	</select>

	<!-- Pagination Total Rows -->
	<select id="PaginationTotalRows" parameterType="Map" resultType="int">
		SELECT count(1) 
		  FROM (SELECT tenant_id 
				  FROM lfitvenda 
				 WHERE tenant_id = #{tenantId,jdbcType=INTEGER}) AS TAGPAGE
	</select>	

	<!-- Fetch Lfitvenda By type -->
	<select id="fetchLfitvendaByType" parameterType="LfitvendaRequest"  resultMap="listAllLfitvendaResults" >
		SELECT  <include refid="allCustomListLfitvendaColumns"/>
		  FROM lfitvenda
		 WHERE name = #{lfitvenda.name,jdbcType=VARCHAR}
		   AND tenant_id = #{tenant.id,jdbcType=INTEGER}
	
	</select>

	<!-- Fetch Lfitvenda By Id -->
	<select id="fetchLfitvendaById" parameterType="Lfitvenda" resultMap="listAllLfitvendaResults" >
		SELECT <include refid="allCustomListLfitvendaColumns"/>
