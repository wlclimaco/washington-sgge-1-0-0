 ConfigSchema.ServicoConNfse  := 'schema_nfse_v03.xsd';
 ConfigSchema.ServicoCancelar := 'schema_nfse_v03.xsd';
 ConfigSchema.DefTipos        := '';

 Result := ConfigSchema;
end;

