{$I ACBr.inc}
package ACBr_NFe2;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'ACBrNFePCN2 - (http://acbr.sourceforge.net/)'}
{$IMPLICITBUILD ON}

requires
  vcl,
  rtl,
  designide,
  xmlrtl,
  dbrtl,
  inet,
  soaprtl,
  ACBrCapicom,
  ACBrOpenSSL,
  synapse,
  PCN2,
  ACBrSerial;

contains
  ACBrNFeConfiguracoes in '..\..\..\Fontes\ACBrNFe2\ACBrNFeConfiguracoes.pas',
  ACBrNFeNotasFiscais in '..\..\..\Fontes\ACBrNFe2\ACBrNFeNotasFiscais.pas',
  ACBrNFeReg in '..\..\..\Fontes\ACBrNFe2\ACBrNFeReg.pas',
  ACBrNFeUtil in '..\..\..\Fontes\ACBrNFe2\ACBrNFeUtil.pas',
  ACBrNFeWebServices in '..\..\..\Fontes\ACBrNFe2\ACBrNFeWebServices.pas',
  ACBrNFeDANFEClass in '..\..\..\Fontes\ACBrNFe2\ACBrNFeDANFEClass.pas',
  ACBrNFe in '..\..\..\Fontes\ACBrNFe2\ACBrNFe.pas';

end.
